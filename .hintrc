{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": "off",
    "apple-touch-icons": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]",
          "img[loading]"
        ]
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 111",
    "not and_ff <= 110",
    "not chrome <= 111",
    "not edge <= 111",
    "not firefox <= 111",
    "not ios_saf <= 16.3",
    "not opera <= 95",
    "not safari <= 16.3",
    "not samsung <= 20"
  ]
}