{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "scroll-behavior"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]",
          "img[loading]"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}